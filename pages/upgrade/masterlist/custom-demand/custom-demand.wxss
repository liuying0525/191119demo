/* pages/upgrade/masterlist/customdemand.wxss */
@import "../../../common/search/search.wxss";
@import "../../../common/menu/menu.wxss";
@import "../../../common/calendar/calendar.wxss";
.custom-demand{
  width:100%;
  height:9%;
  background: #6194c0;  
  position: fixed;
  top: 0;
  left: 0;
  z-index: 20;
  display: flex;
  align-items:center;
  border-bottom:10rpx solid #fc7afc;
  padding:0 3%;
  box-sizing: border-box;
}
.custom-demand-list{
  width:64%;
  background-color: #fff;
  height: 70rpx;
}
.custom-demand-list input{
  width:85%;
  height: 70rpx;
  padding-left: 15rpx;
  display: inline-block;
  vertical-align: middle;
}
.custom-demand-list  image{
  width: 35rpx;
  height: 35rpx;
  vertical-align: middle;
  margin-right: 15rpx;
}
 .search-img{
   display: inline-block;
  color: #fff;
  font-size:24rpx;
  background-color:#4c607d;
  padding:18rpx 18rpx;
vertical-align:middle;

}
.search-img image{
  height:40rpx;
  width:40rpx;
  display:inline-block;
vertical-align:middle;

}
.custom-demand .custon-add {
  margin: 0 22rpx;
  width:70rpx;
  height:70rpx;
  display: inline-block;
  vertical-align: middle;
}
.custom-demand .custom-scree{
  margin: 0 5rpx;
  width: 65rpx;
  height:65rpx;
  float: right;
}
.prompt{
  width:16rpx;
  height:16rpx;
  border-radius: 50%;
  background-color:#ff3e3b;
 display: inline-block;
 vertical-align: super;
}
.main .li{
    position: relative;
}
.state {
  position: absolute;
  top: 10rpx;
  right: 5px;
  display: inline-block;
  line-height:40rpx;
  height: 38rpx;
  font-size:32rpx;
  color: #56b056;
  padding: 0 10rpx;
  border-radius: 5rpx;
  border: 1px solid #56b056;
}

